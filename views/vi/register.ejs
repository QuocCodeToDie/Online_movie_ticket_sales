    <div class="container">
        <form class="form-signup" name="myForm" action="/en/register" onsubmit="return validateForm()" method="POST">
            <br>
            <h1 class="h3 mb-3 font-weight-normal">Đăng ký</h1>

            <div class="row">
                <div class="col-lg-6 col-md-4 col-xs-4">
                    <label for="name" class="sr-only">Tên </label>
                    <input type="text" class="form-control" name="name" id="name" placeholder="Tên" required autofocus>
                </div>
                <div class="col-lg-6 col-md-4 col-xs-4">
                    <input class="form-control" name="lastname" placeholder="Họ" required>
                </div>
            </div>
            <div>
                <label for="email" class="sr-only">Địa chỉ Email</label>
                <input type="email" class="form-control" name="email" id="email" placeholder="Email address" required autofocus>
            </div>
            <div>
                <label for="password" class="sr-only">Mật khẩu</label>
                <input type="password" class="form-control" name="password" id="password" placeholder="Mật khẩu" required>
            </div>
            <!-- <div>
                <input type="password" id="confirmPassword" class="form-control" name="confirmPassword" placeholder="Confirm password" required>
            </div> -->
            <div>
                <label for="inputBirthDate">
                
                <br>Ngày sinh
            </label>
            </div>
            <div class="row">
                <div class="col-lg-2 col-md-2 col-xs-4">
                    <select class="form-control" name="day">
                        <option>Ngày</option>
                        <option value="1">01</option>
                        <option value="2">02</option>
                        <option value="3">03</option>
                        <option value="4">04</option>
                        <option value="5">05</option>
                        <option value="6">06</option>
                        <option value="7">07</option>
                        <option value="8">08</option>
                        <option value="9">09</option>
                        <option value="10">10</option>
                        <option value="11">11</option>
                        <option value="12">12</option>
                        <option value="13">13</option>
                        <option value="14">14</option>
                        <option value="15">15</option>
                        <option value="16">16</option>
                        <option value="17">17</option>
                        <option value="18">18</option>
                        <option value="19">19</option>
                        <option value="20">20</option>
                        <option value="21">21</option>
                        <option value="22">22</option>
                        <option value="23">23</option>
                        <option value="24">24</option>
                        <option value="25">25</option>
                        <option value="26">26</option>
                        <option value="27">27</option>
                        <option value="28">28</option>
                        <option value="29">29</option>
                        <option value="30">30</option>
                        <option value="31">31</option>
                    </select>
                </div>
                <div class="col-lg-2 col-md-2 col-xs-4">
                    <select class="form-control" name="month">
                        <option>Tháng</option>
                        <option value="january">Tháng một</option>
                        <option value="february">Tháng hai</option>
                        <option value="march">Tháng ba</option>
                        <option value="april">Tháng tư</option>
                        <option value="may">Tháng năm</option>
                        <option value="june">Tháng sáu</option>
                        <option value="july">Tháng bảy</option>
                        <option value="august">Tháng tám</option>
                        <option value="september">Tháng chín</option>
                        <option value="october">Tháng mười</option>
                        <option value="november">Tháng mười một</option>
                        <option value="december">Tháng mười hai</option> 
                    </select>
                </div>
                <div class="col-lg-2 col-md-2 col-xs-4">
                    <select class="form-control" name="year">
                        <option>Năm</option>
                        <option value="2010">2010</option>
                        <option value="2009">2009</option>
                        <option value="2008">2008</option>
                        <option value="2007">2007</option>
                        <option value="2006">2006</option>
                        <option value="2005">2005</option>
                        <option value="2004">2004</option>
